{
  "manifest_version": 3,
  "name": "Badge Printer",
  "version": "1.0.0",
  "action": {
    "default_title": "Print badge"
  },
  "permissions": [
    "declarativeContent",
    "scripting"
  ],
  "host_permissions": [
    "https://manage.webconnex.com/"
  ],
  "background": {
    "service_worker": "event_page.js"
  },
  "icons": {
    "16": "icons/icon_16.png",
    "32": "icons/icon_32.png",
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  }
}